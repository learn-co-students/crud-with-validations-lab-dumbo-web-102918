<%= form_for @song do |f| %>
<div><%= f.label :title %></div>
<div><%= f.text_field :title %></div>
<div><%= f.label :released %></div>
<div><%= f.check_box :released %></div>
<div><%= f.label :release_year %></div>
<div><%= f.text_field :release_year %></div>
<div><%= f.label :artist_name %></div>
<div><%= f.text_field :artist_name %></div>
<div><%= f.label :genre %></div>
<div><%= f.text_field :genre %></div>
<div><%= f.submit %></div>
<% end %>
